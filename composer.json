{
  "name": "coisa/doctrine",
  "description": "PSR-11 Doctrine container config provider",
  "license": "Apache-2.0",
  "type": "library",
  "authors": [
    {
      "name": "Felipe Say√£o Lobato Abreu",
      "email": "github@felipeabreu.com.br"
    }
  ],
  "support": {
    "issues": "https://github.com/coisa/dbal/issues",
    "source": "https://github.com/coisa/dbal",
    "rss": "https://github.com/coisa/dbal/releases.atom"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": "^7.1",
    "psr/container": "^1.0",
    "doctrine/dbal": "^2.8",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "doctrine/migrations": "^1.8",
    "friendsofphp/php-cs-fixer": "^2.13",
    "phpunit/phpunit": "^7.3",
    "zendframework/zend-servicemanager": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "CoiSA\\Doctrine\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "zf": {
      "config-provider": "./src/ConfigProvider.php"
    }
  }
}
